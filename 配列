var_a = 0
var_b = 0
list = [3,2,5,6]
while var_a < len(list):
    var_b = var_a + 1
    while var_b < len(list):
        if list[var_a] > list[var_b]:
            var_c = list[var_a]
            list[var_a] = list[var_b]
            list[var_b] = var_c
        
        var_b = var_b + 1
    
    var_a = var_a + 1

print(list)
